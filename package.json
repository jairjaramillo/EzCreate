{
  "name": "ez-create",
  "version": "0.1.4",
  "description": "Javascript tool to easily create HTML elements using Javascript",
  "main": "./dist/bundle.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jairjy/EzCreate.git"
  },
  "keywords": [
    "html",
    "tool",
    "wrapper"
  ],
  "author": "Jair Jaramillo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jairjy/EzCreate/issues"
  },
  "homepage": "https://github.com/jairjy/EzCreate#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-cli": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
